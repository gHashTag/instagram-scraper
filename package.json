{
  "name": "instagram-scraper-bot",
  "version": "1.0.0",
  "description": "Instagram Scraper Bot module for Telegram bot",
  "type": "module",
  "scripts": {
    "dev": "bun run scripts/dev-dashboard.ts",
    "dev:pm2": "pm2 start ecosystem.config.cjs --env development",
    "start:pm2": "pm2 start ecosystem.config.cjs --env production",
    "stop:pm2": "pm2 stop ecosystem.config.cjs",
    "delete:pm2": "pm2 delete ecosystem.config.cjs",
    "logs:pm2": "pm2 logs",
    "db:studio": "bunx drizzle-kit studio --port 3457",
    "db:generate": "bunx drizzle-kit generate",
    "db:generate-node": "dotenv -e .env.development -- node ./node_modules/.bin/drizzle-kit generate",
    "db:push": "bunx drizzle-kit push",
    "db:push-direct": "bunx drizzle-kit push",
    "db:push-node": "dotenv -e .env.development -- node ./node_modules/.bin/drizzle-kit push",
    "db:migrate-node": "dotenv -e .env.development -- node ./node_modules/.bin/drizzle-kit migrate",
    "exec-drizzle-studio": "dotenv -e .env.development -- bunx drizzle-kit studio --port 3457",
    "agent:test-run": "dotenv -e .env.development -- tsx agent/index.ts",
    "test": "bun run scripts/build-and-test.sh",
    "test:ui": "bunx vitest --ui",
    "test:integration": "bun run scripts/test-integration.sh"
  },
  "keywords": [
    "instagram",
    "scraper",
    "telegram",
    "bot"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@neondatabase/serverless": "^1.0.0",
    "apify-client": "^2.12.3",
    "better-sqlite3": "^11.10.0",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.43.1",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "open": "^10.1.2",
    "pg": "^8.15.6",
    "telegraf": "^4.16.3",
    "uuid": "^11.1.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "^1.2.12",
    "@types/express": "^5.0.1",
    "@vitest/ui": "^3.1.3",
    "dotenv-cli": "^8.0.0",
    "tsx": "^4.19.4",
    "vitest": "^3.1.3"
  }
}
